{
  "markdown": {
    "idInHeadings": true
  },
  "opts": {
    "destination": "docs",
    "recurse": true,
    "readme": "./README.md"
  },
  "plugins": [
    "plugins/markdown",
    "./node_modules/jsdoc-export-default-interop/dist/index"
  ],
  "source": {
    "include": [
      "src"
    ]
  },
  "sourceType": "module",
  "templates": {
    "default": {
      "layoutFile": "./layout.tmpl"
    }
  }
}
