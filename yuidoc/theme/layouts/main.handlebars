---
layout: api
---


<div class="apidocs">
  <div id="docs-main">
    <div class="content">
      {{>options}}
      {{>layout_content}}
    </div>
  </div>
</div>

{{>sidebar}}

<script src="{{projectAssets}}/vendor/jquery/jquery-1.8.2.min.js"></script>
<script src="{{projectAssets}}/vendor/bootstrap/js/bootstrap.js"></script>
<script src="{{projectAssets}}/vendor/prettify/prettify-min.js"></script>
<script src="{{projectAssets}}/js/yuidoc-bootstrap.js"></script>

<script>
  window.YUIDocs = {
    classes: []
  };
  {{#publicClasses classes}}
    YUIDocs.classes.push({
      name: '{{name}}',
      url: '{{../projectRoot}}classes/{{name}}.html'
    })
  {{/publicClasses}}
</script>
<script>prettyPrint();</script>
